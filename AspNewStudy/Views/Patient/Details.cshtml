@model AspNewStudy.Models.Patient

<div>
    <h4>Patient</h4>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            Id:
        </dt>
        <dd>
            @Html.DisplayFor(model => model.Id)
        </dd>

        <dt>
            Name:
        </dt>
        <dd>
            @Html.DisplayFor(model => model.account.FirstName)
        </dd>


        <dt>
            LastName:
        </dt>
        <dd>
            @Html.DisplayFor(model => model.account.LastName)
        </dd>

        <dt>
            Name:
        </dt>
        <dd>
            @Html.DisplayFor(model => model.account.CreatedDate)
        </dd>

        <dt>
            Doctors:
        </dt>
        <dd>
            <ul>
                @foreach (AspNewStudy.Models.Doctor p in Model.Doctors)
                {
                    <li>@p.account.FirstName</li>
                }
            </ul>
    </dl>
</div>
<p>
    @Html.ActionLink("Edit", "Edit", new { id = Model.Id }) |
    @Html.ActionLink("Back to List", "Index")
</p>
