@using AspNewStudy.Models;
@model IndexListViewModel<Doctor>



<div>
  
    <div>
        <form method="get">
            <div>
                <div class="form-inline">
                    @Html.LabelFor(d => d.Selects["Patient"] , "Patient", new { @class = "control-label" })
                    @Html.DropDownList("Patient", @Model.Selects["Patient"] as SelectList, htmlAttributes: new { @class = "form-control" })
                </div>
            </div>
            <div>
                <input type="submit" value="Filter" class="btn btn-primary"/>
            </div>
        </form>
    </div>

    <div>
        <table>
            <tr>
                <th>Name</th>
                <th>Lastname</th>
                <th>Gender</th>
            </tr>
            @foreach (var item in Model.Objects)
            {
                <tr>
                    <td>@Html.DisplayFor(ModelItem => item.account.FirstName)</td>
                    <td>@Html.DisplayFor(ModelItem => item.account.LastName)</td>
                    <td>@Html.DisplayFor(ModelItem => item.account.Gender)</td>
                </tr>
            }
        </table>
    </div>

</div>